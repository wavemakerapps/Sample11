    <xs:complexContent mixed="false">
      <xs:extension base="q11:TransactionInfo" xmlns:q11="http://schemas.datacontract.org/2004/07/Radixx.ConnectPoint.Request">
        <xs:sequence>
          <xs:element name="ActionType" type="tns:ModifyPNR.ActionTypes">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Action type indicating the option to add or remove flight, passenger or special services.</xs:documentation>
              <xs:documentation>Constraints: Required.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReservationInfo" nillable="true" type="q11:ReservationInfo">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Reservation Information must be populated.</xs:documentation>
              <xs:documentation>Constraints: Required.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FareInformationID" type="xs:int">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: The value of this field is returned in the Fare Quote Response. It will be the FareID of the flight chosen by the consumer via the User Interface.</xs:documentation>
              <xs:documentation>Constraints: Required.</xs:documentation>
              <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LogicalFlightKeyToReplace" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Logical flight key to replace.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LogicalFlightKeyToRemove" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Logical flight key to remove.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PersonOrgIdentifierToRemove" type="xs:int">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Identifier of the passenger to be removed. The value of this property can be obtained from the AirLine Person nod of ViewPNR.</xs:documentation>
              <xs:documentation>Constraints: Required.</xs:documentation>
              <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PassengersToAdd" nillable="true" type="tns:ArrayOfModifyPNR.Passenger">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Collection of passengers.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
              <xs:documentation>Range: Value must be in the range 0 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PassengersToUpdate" nillable="true" type="tns:ArrayOfModifyPNR.UpdatePassenger">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Collection of passengers who are in need of name updates.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
              <xs:documentation>Range: Value must be in the range 0 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SpecialServices" nillable="true" type="tns:ArrayOfSpecialService">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Collection of special services.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
              <xs:documentation>Range: Value must be in the range 0 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SpecialServicesToCancel" nillable="true" type="tns:ArrayOfSpecialServiceToCancel">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Collection of special services to be removed/cancelled from a PNR.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
              <xs:documentation>Range: Value must be in the range 0 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ContactInformationToAdd" nillable="true" type="tns:ArrayOfModifyPNR.AddContactInfo">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Collection of contact information that are in need of being added.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
              <xs:documentation>Range: Value must be in the range 0 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ContactInformationToUpdate" nillable="true" type="tns:ArrayOfModifyPNR.UpdateContactInfo">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Collection of contact information that are in need of updates.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
              <xs:documentation>Range: Value must be in the range 0 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CommentToAdd" nillable="true" type="tns:Comment">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Comment that will be added to the PNR.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
              <xs:documentation>Range: Value must be in the range 0 and 1.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="EmergencyContactsToAddToSegments" nillable="true" type="tns:ArrayOfModifyPNR.EmergencyContact">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Collection of Emergency Contacts to add to passenger segments.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
              <xs:documentation>Range: Value must be in the range 0 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="PassengerSegmentsToUpdateRecordLocator" nillable="true" type="tns:ArrayOfModifyPNR.UpdateRecordLocator">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Collection of Emergency Contacts to add to passenger segments.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
              <xs:documentation>Range: Value must be in the range 0 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="ChangeFlightFromReservation" nillable="true" type="tns:ModifyPNR.OriginDestinations"/>
          <xs:element minOccurs="0" name="Channel" nillable="true" type="xs:string"/>
          <xs:element minOccurs="0" name="SubChannel" nillable="true" type="xs:string"/>
          <xs:element minOccurs="0" name="PointOfSale" nillable="true" type="xs:string"/>
          <xs:element minOccurs="0" name="Currency" nillable="true" type="xs:string"/>
          <xs:element minOccurs="0" name="PaxToAdd" nillable="true" type="tns:ModifyPNR.modifyCPRequest">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Collection of passengers.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
              <xs:documentation>Range: Value must be in the range 0 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="ModifyPNR" nillable="true" type="tns:ModifyPNR"/>
  <xs:simpleType name="ModifyPNR.ActionTypes">
    <xs:annotation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="AddFlightToReservation">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to add a flight to a reservation&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="RemoveFlightFromReservation">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to remove a flight and all passengers within from a reservation&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ReplaceFlightOnReservation">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to replace a flight on a reservation&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AddPassengerToReservation">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to add a passenger to a reservation&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="RemovePassengerFromReservation">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to remove a passenger from a reservation&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="UpdatePassengerInformationNoFee">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to update passenger name information without a system defined fee being assessed&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="UpdatePassengerInformationAssessFee">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to update passenger name information with a system defined fee being assessed&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AddSpecialServiceToReservation">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to add a special service to a reservation&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="RemoveSpecialServiceFromReservation">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to add a special service to a reservation&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AddContactInformation">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to add contact Information&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="UpdateContactInformation">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to update contact Information&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AddCommentToReservation">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to add comment to a reservation&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AddEmergencyContact">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to add an emergency contact to a passenger segment&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="UpdatePassengerSegmentRecordLocator">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to update the record loactor of a passenger segment&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ChangeFlightFromReservation">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to change flight&lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ChangeFlightCabin">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to add a passenger to a reservation&lt;/summary>
 Created at the time of Repricer</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AddPaxToReservation">
        <xs:annotation>
          <xs:documentation>&lt;summary>Action to add a passenger to a reservation&lt;/summary>
 Created at the time of Repricer</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="ModifyPNR.ActionTypes" nillable="true" type="tns:ModifyPNR.ActionTypes"/>
  <xs:complexType name="ArrayOfModifyPNR.Passenger">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="ModifyPNR.Passenger" nillable="true" type="tns:ModifyPNR.Passenger"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfModifyPNR.Passenger" nillable="true" type="tns:ArrayOfModifyPNR.Passenger"/>
  <xs:complexType name="ModifyPNR.Passenger">
    <xs:complexContent mixed="false">
      <xs:extension base="tns:ModifyPNR.Person">
        <xs:sequence>
          <xs:element minOccurs="0" name="DropOffID" type="xs:int">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: The PersonOrgID of the individual that is authorized to drop off an unaccompanied minor..</xs:documentation>
              <xs:documentation>Constraints: Required.</xs:documentation>
              <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="PickUpID" type="xs:int">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: The PersonOrgID of the individual that is authorized to pick up an unaccompanied minor..</xs:documentation>
              <xs:documentation>Constraints: Required.</xs:documentation>
              <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="LapChildID" type="xs:int">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: The PersonOrgID of the passenger who is going to be carrying an Infant on his/her lap..</xs:documentation>
              <xs:documentation>Constraints: Required.</xs:documentation>
              <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="FrequentFlyerNumber" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: The frequent flyer number for this passenger as entered in the DCS.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
              <xs:documentation>Range: Length must be in the range 1 and 20.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="SelecteeStatus" type="xs:int">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: A range of values that describes the passenger’s identification under various governmental security regimes.</xs:documentation>
              <xs:documentation>Constraints: Required.</xs:documentation>
              <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="StoreFrontID" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: In the event that an airline has multiple storefronts, this field indicates the StoreFrontID that this booking was made under.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TravelsWithPersonOrgID" type="xs:int">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: If an infant is traveling with an adult then the infants person org id will be populated in this property.</xs:documentation>
              <xs:documentation>Constraints: Required.</xs:documentation>
              <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RedressNumber" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Secure Flight Information.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="KnownTravelerNumber" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Secure Flight Information.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="RecordLocator" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: If this field is populated then the recordlocator passed in will be populated in the reservation segment of the person that it is passed under.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TierID" type="xs:int">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Tier ID.</xs:documentation>
              <xs:documentation>Constraints: Required.</xs:documentation>
              <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="TierName" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Tier Name.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="FlightInformations" nillable="true" type="tns:ArrayOfModifyPNR.FlightInformation">
            <xs:annotation>
              <xs:appinfo/>
              <xs:documentation>Description: Collection of flight information.</xs:documentation>
              <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
              <xs:documentation>Range: Value must be in the range 1 and 2147483647.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="ModifyPNR.Passenger" nillable="true" type="tns:ModifyPNR.Passenger"/>
  <xs:complexType name="ModifyPNR.Person">
    <xs:sequence>
      <xs:element name="PersonOrgID" type="xs:int">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The automatically system generated unique identifier for the passenger. Unless Performing a passenger Name Change then it will be the personOrgID rendered from the ViewPNR Response.</xs:documentation>
          <xs:documentation>Constraints: Required.</xs:documentation>
          <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FirstName" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The passenger's first name..</xs:documentation>
          <xs:documentation>Constraints: Required.</xs:documentation>
          <xs:documentation>Range: Length must be in the range 1 and 64.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LastName" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The passenger's last name..</xs:documentation>
          <xs:documentation>Constraints: Required.</xs:documentation>
          <xs:documentation>Range: Length must be in the range 1 and 64.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MiddleName" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The passenger's middle name..</xs:documentation>
          <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
          <xs:documentation>Range: Length must be in the range 1 and 64.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Age" type="xs:int">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The age of the passenger at the time of the booking..</xs:documentation>
          <xs:documentation>Constraints: Required.</xs:documentation>
          <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DOB" type="xs:dateTime">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The passenger's date of birth..</xs:documentation>
          <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Gender" type="q12:Enumerations.GenderTypes" xmlns:q12="http://schemas.datacontract.org/2004/07/Radixx.ConnectPoint.Request">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The code specifying the gender of the passenger..</xs:documentation>
          <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Title" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The salutation for the passenger (e.g. Mr., Miss.)..</xs:documentation>
          <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
          <xs:documentation>Range: Length must be in the range 1 and 20.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NationalityLaguageID" type="xs:int">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The unique identifier that represents the passenger's native language..</xs:documentation>
          <xs:documentation>Constraints: Required.</xs:documentation>
          <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RelationType" type="q13:Enumerations.RelationshipTypes" xmlns:q13="http://schemas.datacontract.org/2004/07/Radixx.ConnectPoint.Request">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The relation of the person booking the flight to that of the passenger..</xs:documentation>
          <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="WBCID" type="xs:int">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: Wieght and Balance Passenger Type Code ID for this passenger.</xs:documentation>
          <xs:documentation>Constraints: Required.</xs:documentation>
          <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PTCID" type="xs:int">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The code representing the Passenger Type Code for the passenger segment..</xs:documentation>
          <xs:documentation>Constraints: Required.</xs:documentation>
          <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PTC" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The iata standard code representing the Passenger Type Code for the passenger segment. (industry predefined Codes ADT, INF, CHD).</xs:documentation>
          <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UseInventory" type="xs:boolean">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: Indicates whether the passenger uses an inventory space..</xs:documentation>
          <xs:documentation>Constraints: Required.</xs:documentation>
          <xs:documentation>Range: Value must be in the range 1 and 3.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Address" nillable="true" type="tns:Address">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The passenger's address..</xs:documentation>
          <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Company" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The company where the passenger works..</xs:documentation>
          <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Comments" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: A comment for the passenger..</xs:documentation>
          <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Passport" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The passenger's passport number..</xs:documentation>
          <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Nationality" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The nationality of the passenger..</xs:documentation>
          <xs:documentation>Constraints: Optional (Not Required).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ProfileId" type="xs:long">
        <xs:annotation>
          <xs:appinfo/>
          <xs:documentation>Description: The Radixx profile ID that is associated with this reservation.</xs:documentation>
          <xs:documentation>Constraints: Required.</xs:documentation>
          <xs:documentation>Range: Value must be in the range -2147483648 and 2147483647.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ContactInfos" nillable="true" type="tns:ArrayOfContactInfo">
        <xs:annotation>
          <xs:appinfo/>
